{
    "name": "kdn/yii2-json-editor",
    "description": "JSON editor widget (josdejong/jsoneditor) for Yii 2.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "widget",
        "editor",
        "json",
        "jsoneditor",
        "viewer",
        "formatter"
    ],
    "readme": "./README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Kulikov",
            "email": "kulikovdn@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/dmitry-kulikov/yii2-json-editor/issues",
        "source": "https://github.com/dmitry-kulikov/yii2-json-editor"
    },
    "require": {
        "php": ">=5.4.0",

        "npm-asset/jsoneditor": ">=5.0 <10.0",
        "yiisoft/yii2": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0 <10.0",
        "phpunit/php-invoker": ">=1.0 <4.0"
    },
    "suggest": {
        "ext-runkit": "this extension is needed to run all tests, otherwise some tests will be skipped"
    },
    "autoload": {
        "psr-4": {
            "kdn\\yii2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kdn\\yii2\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "bower-asset-library": "vendor/bower",
                "npm-asset-library": "vendor/npm"
            },
            "pattern-skip-version": "(-build|-patch)",
            "repositories": {
                "npm-asset/ajv": {
                    "type": "npm-vcs",
                    "url": "https://github.com/ajv-validator/ajv.git",
                    "no-api": true
                },
                "npm-asset/babel-runtime": {
                    "type": "npm-vcs",
                    "url": "https://github.com/babel/babel.git",
                    "no-api": true
                },
                "npm-asset/brace": {
                    "type": "npm-vcs",
                    "url": "https://github.com/thlorenz/brace.git",
                    "no-api": true
                },
                "npm-asset/fast-json-stable-stringify": {
                    "type": "npm-vcs",
                    "url": "https://github.com/epoberezkin/fast-json-stable-stringify.git",
                    "no-api": true
                },
                "npm-asset/jmespath": {
                    "type": "npm-vcs",
                    "url": "https://github.com/jmespath/jmespath.js.git",
                    "no-api": true
                }
            },
            "vcs-driver-options": {
                "github-no-api": true
            }
        }
    }
}
